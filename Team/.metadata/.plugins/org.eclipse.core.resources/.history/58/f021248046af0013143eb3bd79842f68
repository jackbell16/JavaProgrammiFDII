/**
 * Questa classe astrae sul concetto di Attivit√†
 */
package utils.log;

public class Attivita {
	
	private int ora;
	private String nome;
	
	public Attivita(int ora, String nome) {
		super();
		this.ora = ora;
		this.nome = nome;
	}
	public int getOra() {
		return ora;
	}
	public void setOra(int ora) {
		this.ora = ora;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
}
