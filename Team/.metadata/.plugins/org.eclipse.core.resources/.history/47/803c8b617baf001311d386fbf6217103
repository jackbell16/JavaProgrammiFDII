public class Tests04 {
	public static void main(String[] args) {
		ArrayList<Attivita> list = new ArrayList<>();
		Attivita attivita = new Attivita(10, "Prova");
		Attivita attivita2 = new Attivita(30, "Prova1");
		Attivita attivita3 = new Attivita(40, "Prova2");
