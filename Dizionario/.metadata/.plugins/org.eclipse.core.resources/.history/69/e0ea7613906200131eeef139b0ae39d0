package tests;

import utils.CaricaDizionario;
import utils.Dizionario;

public class Test {
	public static void main(String[] args) {
		Dizionario dizionario = new Dizionario();
		dizionario.setCorrispondenza(Dizionario.corrispondenza, "ciao","hello");
		System.out.println(dizionario.getCorrispondenza().get("ciao"));
		for (int i = 0; i < CaricaDizionario.list.size(); i++) {
			System.out.println(CaricaDizionario.list.get(i));
		}
	}
	

}
