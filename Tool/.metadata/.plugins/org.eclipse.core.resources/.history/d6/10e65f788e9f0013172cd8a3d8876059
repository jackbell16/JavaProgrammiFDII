package tests;

import filtri.utils.Info;
import utils.CaricamentoFile;

public class Tests03 {
	public static void main(String[] args) {
		CaricamentoFile file = new CaricamentoFile();
		file.setNomeFile("prova.txt");
		Info info = new Info();
		info.filtra(file.getList());
		for (int i = 0; i < info.getRisultato().size(); i++) {
			System.out.println(info.getRisultato().get(i));
		}
	}
}
