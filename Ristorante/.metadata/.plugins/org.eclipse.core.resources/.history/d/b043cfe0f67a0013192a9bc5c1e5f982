package utils;

public class MenuPrezzoFissoTest implements MenuPrezzoFisso {
	
	private static double costoMenuFisso = 8;
	
	public void applicaMenuPrezzoFisso(double totale){
		totale = costoMenuFisso;
	}
	
	public boolean verificaMenuPrezzoFisso(int numeroPanini,int numeroBibite){
		if(numeroPanini==1 && numeroBibite==1){
			return true;
		}else{
			return false;
		}
	}	
}
