/**
 * Questa classe ha la reponsabilità di contenere le proprietà dell'oggetto Panino
 * @author Giacomo
 */
package utils;

public class Panino implements ProdottoPaninoteca  {
	private String nomePanino;
	private double prezzo;
	public Panino(String nomePanino, double prezzo) {
		super();
		this.nomePanino = nomePanino;
		this.prezzo = prezzo;
	}
	@Override
	public String getNome() {
		return nomePanino;
	}
	@Override
	public double getPrezzo() {
		return prezzo;
	}
	@Override
	public void setNome(String nomeProdotto) {
		this.nomePanino = nomeProdotto;
		
	}
	@Override
	public void setPrezzo(double prezzo) {
		this.prezzo = prezzo;
	}
}
